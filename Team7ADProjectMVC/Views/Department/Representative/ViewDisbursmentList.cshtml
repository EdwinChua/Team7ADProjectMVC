@model Team7ADProjectMVC.DisbursementList
@{
    ViewBag.Title = "Index";
    Layout = "~/_Layout1.cshtml";
}

<h2>View Disbursement List</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.OrderedDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CollectionPoint.PlaceName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Department.DepartmentCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Retrieval.RetrievalId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.OrderedDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CollectionPoint.PlaceName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Department.DepartmentCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Retrieval.RetrievalId)
        </td>
        <td>

            @Html.ActionLink("Details", "Details", new { id=item.DisbursementListId }) |

        </td>
    </tr>
}

</table>
